{
    "name": "ipanta/laravel-pay",
    "description": "专注 Alipay 和 WeChat 的 laravel 支付扩展包",
    "keywords": ["alipay", "wechat", "pay", "laravel", "package"],
    "support": {
        "issues": "https://github.com/ipanta/laravel-pay/issues",
        "source": "https://github.com/ipanta/laravel-pay"
    },
    "authors": [
        {
            "name": "yansongda",
            "email": "me@yansongda.cn"
        }
    ],
    "require": {
        "illuminate/support": "^5.1 || ^6.0 || ^7.0 || ^8.0",
        "ipanta/pay": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ipanta\\LaravelPay\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ipanta\\LaravelPay\\PayServiceProvider"
            ],
            "aliases": {
                "Pay": "Ipanta\\LaravelPay\\Facades\\Pay"
            }
        }
    },
    "license": "MIT"
}
